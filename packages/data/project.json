{
  "name": "data",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/data/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-script",
      "dependsOn": ["static-module"],
      "options": {
        "script" : "pbts -- -o ./src/index.d.ts ./src/index.js"
      }
    },
    "static-module": {
      "executor": "nx:run-script",
      "options": {
        "script" : "pbjs -- -t static-module -w commonjs -o ./src/index.js ./src/main.proto"
      }
    }
  },
  "tags": []
}
